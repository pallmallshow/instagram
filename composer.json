{
    "name": "vinkla/instagram",
    "description": "An Instagram bridge for Laravel",
    "license": "MIT",
    "keywords": ["laravel", "instagram", "images", "api", "image", "media", "facebook"],
    "authors": [
        {
            "name": "Vincent Klaiber",
            "email": "hello@vinkla.com"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/ScottPolhemus/Instagram-PHP-API"
        }
    ],
    "require": {
        "php": "^5.5.9 || ^7.0",
        "illuminate/contracts": "5.1.* || 5.2.*",
        "illuminate/support": "5.1.* || 5.2.*",
        "graham-campbell/manager": "^2.3",
        "cosenary/instagram": "dev-auth"
    },
    "require-dev": {
        "graham-campbell/testbench": "^3.1",
        "mockery/mockery": "^0.9.4",
        "phpunit/phpunit": "^4.8 || ^5.0"
    },
    "autoload": {
        "psr-4": {
            "Vinkla\\Instagram\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Vinkla\\Tests\\Instagram\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.2-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
